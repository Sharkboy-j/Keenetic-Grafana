{
  "metrics" : [
    {
      "command": "processes",
      "root" : "$.process.[*]",
      "tags" : {
        "name": "$.comm",
        "pid": "$.pid"
      },
      "values" : {
        "cpu": "$.statistics.cpu.cur",
        "memory": "$.vm-size"
      }
    },
    {
      "command": "interface",
      "root" : "$",
      "param" : {
        "name" : "GigabitEthernet0/8"
      },
      "tags" : {},
      "values" : {
        "temperature": "$.sfp.temperature",
        "vcc": "$.sfp.vcc",
        "tx-bias": "$.sfp.tx-bias",
        "tx-power": "$.sfp.tx-power",
        "tx-power-mw": "$.sfp.tx-power-mw",
        "rx-power": "$.sfp.rx-power",
        "rx-power-mw": "$.sfp.rx-power-mw",
        "wave-length": "$.sfp.fibre.wave-length",
        "bitrate": "$.sfp.bitrate"
      }
    },
    {
      "command": "ip hotspot",
      "root" : "$.host[*]",
      "tags" : {
        "hostname" : "$.hostname",
        "name" : "$.name",
        "ssid": "$.ssid",
        "mode": "$.mode",
        "ip": "$.ip",
        "mac": "$.mac"
      },
      "values" : {
        "rxbytes": "$.rxbytes",
        "txbytes": "$.txbytes",
        "txrate": "$.txrate",
        "uptime": "$.uptime",
        "signal": "$.rssi",
        "speed": "$.speed",
        "active": "$.active",
        "link": "$.link"
      }
    },
    {
      "command": "ip hotspot",
      "root" : "$.*.host.[*]",
      "post" : "eyJzdW1tYXJ5Ijp7ImF0dHJpYnV0ZSI6InN1bXNwZWVkIn19",
      "tags" : {
        "name" : "$.name",
        "active": "$.active",
        "mac": "$.mac"
      },
      "values" : {
        "sumspeed": "$.sumspeed",
        "rxspeed": "$.rxspeed",
        "txspeed": "$.txspeed"
      }
    },
    {
      "command": "interface",
      "root" : "$.*",
      "tags" : {
        "id": "$.id",
        "type": "$.type",
        "description": "$.description",
        "interface-name": "$.interface-name",
        "name": "$.name",
        "address": "$.address"
      },
      "values" : {
        "mtu": "$.mtu",
        "uptime": "$.uptime",
        "tx-queue": "$.tx-queue",
        "state": "$.state"
      }
    },
    {
      "command": "system",
      "root" : "$",
      "tags" : {
        "hostname" : "$.hostname"
      },
      "values" : {
        "cpuload": "$.cpuload",
        "memtotal": "$.memtotal",
        "memfree": "$.memfree",
        "memcache": "$.memcache",
        "swaptotal": "$.swaptotal",
        "swapfree": "$.swapfree",
        "uptime": "$.uptime",
        "membuffers": "$.membuffers"
      }
    },
    {
      "command": "internet status",
      "root" : "$",
      "tags" : {},
      "values" : {
        "internet": "$.internet",
        "gateway-accessible": "$.gateway-accessible",
        "dns-accessible": "$.dns-accessible",
        "host-accessible": "$.host-accessible",
        "captive-accessible": "$.captive-accessible"
      }
    },
    {
      "command": "media",
      "root" : "$.*.partition.[*]",
      "tags" : {
        "label" : "$.label",
        "uuid" : "$.uuid"
      },
      "values" : {
        "total": "$.total",
        "free": "$.free",
        "state" : "$.state"
      }
    },
    {
      "command": "interface stat",
      "root" : "$",
      "param" : {
        "name" : "PPPoE0"
      },
      "tags" : {},
      "values" : {
        "rxbytes": "$.rxbytes",
        "txbytes": "$.txbytes",
        "rxspeed": "$.rxspeed",
        "txspeed": "$.txspeed"
      }
    },
    {
      "command": "interface stat",
      "root" : "$",
      "param" : {
        "name" : "PPPoE1"
      },
      "tags" : {},
      "values" : {
        "rxbytes": "$.rxbytes",
        "txbytes": "$.txbytes",
        "rxspeed": "$.rxspeed",
        "txspeed": "$.txspeed"
      }
    },
    {
      "command": "interface stat",
      "root" : "$",
      "param" : {
        "name" : "OpenVPN0"
      },
      "tags" : {},
      "values" : {
        "rxbytes": "$.rxbytes",
        "txbytes": "$.txbytes",
        "rxspeed": "$.rxspeed",
        "txspeed": "$.txspeed"
      }
    }
  ]
}